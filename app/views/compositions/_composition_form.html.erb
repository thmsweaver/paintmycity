<%= form_for [@user, @composition] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :latitude %>
  <%= f.number_field :latitude, :step => 'any' %>

  <%= f.label :longitude %>
  <%= f.number_field :longitude, :step => 'any' %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.submit "Submit", id: 'submit' %>
<% end %>
